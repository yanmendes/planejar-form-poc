{
  "name": "ts-express-template",
  "version": "1.0.0",
  "description": "typescript-express-template",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run serve",
    "dev": "npm run serve:w",
    "tsc:s": "npm run tsc:b & npm run tsc:w",
    "tsc:b": "tsc -p tsconfig.json",
    "tsc:w": "tsc -w -p tsconfig.json",
    "serve": "node dist/index.js",
    "serve:w": "nodemon dist/index.js",
    "browser-sync": "browser-sync start --server --files 'dist/*.js'",
    "build": "npm run tsc:b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jinhduong/ts-express-template.git"
  },
