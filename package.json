  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "browser-sync": "^2.24.5",
    "nodemon": "^1.17.5"
